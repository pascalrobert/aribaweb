<a:RefreshRegion>
    <a:ClientSideScript>
        google.pageTracker._trackPageview('$trackPageName');
        <a:If ifTrue="$=ariba.ui.aribaweb.core.AWConcreteServerApplication.IsDebuggingEnabled">
            var windowTitle = '<a:String value="$trackPageName"/>';
            ariba.Debug.setWindowTitle(windowTitle);
        </a:If>        
    </a:ClientSideScript>
</a:RefreshRegion>