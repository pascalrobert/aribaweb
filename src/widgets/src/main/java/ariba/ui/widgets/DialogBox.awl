<a:If ifTrue="$^usesPanelContainer">\
    <table id="$^id" class="$^class" style="$^style"><tr><td class="panel">
        <div class="$^wrapperClass:dialogWrapper" style="$^wrapperStyle">\
            <a:IncludeContent/>\
        </div>\
    </td></tr></table>\
<a:Else/>\
    <div id="$^id" class="$^class" style="$^style">
        <div class="$^wrapperClass:dialogWrapper" style="$^wrapperStyle">\
            <a:IncludeContent/>\
        </div>\
    </div>\
</a:If>\

