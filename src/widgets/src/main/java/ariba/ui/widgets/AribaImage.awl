<a:Image filename="$filename"
         name="$^name"
         width="$^width"
         height="$^height"
         vspace="$^vspace"
         hspace="$^hspace"
         border="$^border"
         title="$^title:$^alt"
         alt="$^alt"
         align="$^align"
         class="$^class"
         onMouseOver="$^onMouseOver"
         onMouseOut="$^onMouseOut"
         onMouseDown="$^onMouseDown"
         onClick="$^onClick"
         style="$^style"/>\
